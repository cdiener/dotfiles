!Xft.dpi: 96
!Xft.antialias: true
!Xft.hinting: true
!Xft.rgba: rgb
!Xft.autohint: false
!Xft.hintstyle: hintslight
!Xft.lcdfilter: lcddefault

! urxvt config
URxvt.iconFile: /usr/share/icons/Tango/32x32/apps/gnome-terminal.png
URxvt.perl-ext-common: default
URxvt.url-launcher: /usr/bin/chromium
!URxvt.matcher.button: 1
URxvt.depth: 32
URxvt.background: [70]#000000
URxvt.foreground: #ffffff
URxvt.scrollBar: false
URxvt.savelines: 6400
URxvt.cursorBlink: 1 
URxvt.internalBorder: 0
URxvt.font: xft:DejaVu Sans Mono:size=9.5
URxvt.letterSpace: 0
URxvt.geometry: 80x20

! Color definition
! black
URxvt*color15  : #66666a
URxvt*color7  : #30303a

! red
URxvt*color9: #ff6699
URxvt*color1: #a3506c

! yellow/orange
URxvt*color11: #ffff00
URxvt*color3: #ff9933

! purple
URxvt*color13: #cc3399
URxvt*color5: #cc0099

! blue
URxvt*color12: #3399ff
URxvt*color4: #0066ff

! green
URxvt*color10 : #99FF33
URxvt*color2  : #6BB224

! cyan
URxvt*color14 : #33ccff
URxvt*color6  : #0099ff

! white
URxvt*color0 : #b1b8c3
URxvt*color8  : #cdcdcd
